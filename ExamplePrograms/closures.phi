# Phi supports closures. All of these approaches are valid

fn One() {
    fn makeAdd() {
        fn add(a, b) {
            <- a + b
        }
        <- add
    }

    lambda x = makeAdd()
    out(x(3, 2))
}

fn Two() {
    fn makeAdd() {
        <- fn add(a, b) {
            <- a + b
        }
    }

    lambda x = makeAdd()
    out(x(3, 2))
}

fn Three() {
    lambda x = fn add(a, b){<- a + b}
    out(x(3, 2))
}

One()
Two()
Three()

