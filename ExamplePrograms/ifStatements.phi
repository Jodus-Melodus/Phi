# Phi includes if statements

var a = 4
var b = 2

if (a == b) {
    out("a is equal to b")
} else {
    out("a is not equal to b")
}

if (a > b) {
    out("a is larger than b")
} else {
    if (a < b) {
        out("a is smaller than b")
    } else {
        out("a is equal to b")
    }
}

if (a == b) {
    out("a is equal to b")
}

