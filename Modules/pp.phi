export obj pp = {
	obj_:fn ~(object){
		str result = "{\"
		array key = object.keys()
		array values = object.items()

		for (int i, i < key.length(), i += 1){
			result += key[i]
			result += ":"
			result += values[i]
			result += "\"
		}

		<- result + "}"
	},
	array_:fn ~(arr){
		str result = "[\"
		for (int i, i < arr.length(), i += 1){
			result += arr[i]
			result += "\"
		}
		<- result + "]"
	}
}